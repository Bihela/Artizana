{
  "expo": {
    "name": "artizana-mobile",
    "slug": "artizana-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "artizana-dev",
    "projectId": "95ed859a-b147-43f0-a7a0-b4fa060a2a9a",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.artizana.mobile",
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin"
    ],
    "extra": {
      "apiBaseUrl": "http://192.168.8.101:5000/api",
      "eas": {
        "projectId": "95ed859a-b147-43f0-a7a0-b4fa060a2a9a"
      }
    }
  }
}